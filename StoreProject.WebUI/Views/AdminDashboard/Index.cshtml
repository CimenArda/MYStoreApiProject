
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<div class="row">
    <div class="col-md-6 grid-margin stretch-card">
        <div class="card tale-bg">
            <div class="card-people mt-auto">
                <img src="~/skydash/images/dashboard/people.svg" alt="people">
                <div class="weather-info">
                    <div class="d-flex">
                        <div>
                            <h2 class="mb-0 font-weight-normal"><i class="icon-sun mr-2"></i>31<sup>C</sup></h2>
                        </div>
                        <div class="ml-2">
                            <h4 class="location font-weight-normal">İstanbul</h4>
                            <h6 class="font-weight-normal">Turkiye</h6>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
    @await Component.InvokeAsync("_AdminDashboardMiniStatisticComponentPartial")
</div>


<div class="row">
    @await Component.InvokeAsync("_AdminDashboardContactComponentPartial")
    @await Component.InvokeAsync("_AdminDashboardCategoryComponentPartial")

    
</div>




<div class="row">
    @await Component.InvokeAsync("_AdminDashboardProductComponentPartial")

    
</div>
             
